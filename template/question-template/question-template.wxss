.ti .ti-wen {
  font-size: 32rpx;
  line-height: 44rpx;
  margin-bottom: 40rpx;
}

.ti .ti-wen .ti-wen-num {
  background-color: #ddd;
  width: 40rpx;
  height: 40rpx;
  display: inline-block;
  text-align: center;
  margin-right: 5px;
  font-size: 24rpx;
  line-height: 40rpx;
}

.ti .ti-da {
  font-size: 30rpx;
}

.ti .ti-da radio-group label {
  display: block;
  /* margin-bottom: 10px; */
  padding: 5px 0;
}
.que-img{display:inline-block;text-align:center;width:250rpx;margin:5px 2px;}
.que-img image{height:250rpx;width:250rpx;}
.que-img-num{display:block;text-align:center;}


.jiexi{font-size:32rpx;font-weight:initial;}
.jiexi-da{display:flex;margin-bottom:10px;padding-top:15px;border-top:1px dashed #ddd;}
.jiexi-da-l{flex:1;}
.jiexi-da-c{flex:1;}
.jiexi-da-r{flex:1;text-align:right;}
.ti .ti-right .jiexi-da-r text{background-color:#97c732;color:#fff;padding:2px 5px;border-radius:2px;font-weight:initial;}
.jiexi-li{display:flex;margin-bottom:10px;}
.jiexi-li-l{flex:1;}
.jiexi-li-r{flex:1;text-align:right;}
.ti-resolve{font-size:32rpx;}

.AA{
  color: #0b8cf9;
}
.BB{
  color:red;
}

.tjda{
  text-align:center;
  background-color:#0b8cf9;
  color:#fff;
  line-height:75rpx;
  margin-top:15px;
}








